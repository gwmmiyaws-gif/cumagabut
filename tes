local WindUI = loadstring(game:HttpGet("https://raw.githubusercontent.com/Footagesus/WindUI/main/dist/main.lua"))()

local svc = {
    RS = game:GetService("ReplicatedStorage"),
}

local mods = {
    Net = svc.RS.Packages._Index["sleitnick_net@0.2.0"].net,
}

local api = {
    Functions = {
        ChargeRod = mods.Net["RF/ChargeFishingRod"],
        StartMini = mods.Net["RF/RequestFishingMinigameStarted"],
        CatchFish = mods.Net["RF/CatchFishCompleted"],
        CancelFish = mods.Net["RF/CancelFishingInputs"],
    },
}

local Window = WindUI:CreateWindow({
    Title = "Auto Fish",
    Author = "Patungin",
    Folder = "AutoFish",
    Size = UDim2.fromOffset(400, 300),
    ToggleKey = Enum.KeyCode.G,
})

local Main = Window:Tab({
    Title = "Main",
    Icon = "fish",
})

local Running = false
_G.DelayComplete = 0.001  -- Super fast
_G.DelayCycle = 0.05      -- Cepet banget cycle
_G.SpamAmount = 200       -- Banyak spam

local function AutoFish()
    while Running do
        pcall(function()
            -- FULL SPAM ChargeRod + StartMini
            for i = 1, _G.SpamAmount do
                if not Running then break end
                
                local now = workspace:GetServerTimeNow()
                
                -- Spam ChargeRod
                pcall(function()
                    api.Functions.ChargeRod:InvokeServer(now + (i * 0.0000001))
                end)
                
                -- Spam StartMini
                pcall(function()
                    api.Functions.StartMini:InvokeServer(-1, 1, now + (i * 0.0000001))
                end)
                
                task.wait(_G.DelayComplete)
            end
            
            -- Complete dengan Catch & Cancel
            pcall(function()
                api.Functions.CatchFish:InvokeServer()
            end)
            
            task.wait(0.01)
            
            pcall(function()
                api.Functions.CancelFish:InvokeServer()
            end)
            
            -- Tunggu sebelum cycle berikutnya
            task.wait(_G.DelayCycle)
        end)
    end
end

Main:Toggle({
    Title = "Start Auto Fish",
    Default = false,
    Callback = function(state)
        Running = state
        if state then
            task.spawn(AutoFish)
        end
    end,
})

Main:Input({
    Title = "Delay Complete",
    Description = "Delay antara spam (detik)",
    Value = "0.001",
    Callback = function(v)
        local n = tonumber(v)
        if n and n >= 0.0001 then
            _G.DelayComplete = n
        end
    end,
})

Main:Input({
    Title = "Delay Cycle",
    Description = "Delay per cycle (detik)",
    Value = "0.05",
    Callback = function(v)
        local n = tonumber(v)
        if n and n >= 0.001 then
            _G.DelayCycle = n
        end
    end,
})

Main:Input({
    Title = "Spam Amount",
    Description = "Jumlah spam per cycle",
    Value = "200",
    Callback = function(v)
        local n = tonumber(v)
        if n and n > 0 then
            _G.SpamAmount = n
        end
    end,
})

print("ðŸŽ£ Auto Fish Loaded! FULL SPAM MODE")

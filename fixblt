local WindUI = loadstring(game:HttpGet("https://raw.githubusercontent.com/Footagesus/WindUI/main/dist/main.lua"))()

wait(2)

local svc = {
    Players = game:GetService("Players"),
    RS = game:GetService("ReplicatedStorage"),
}

local player = svc.Players.LocalPlayer

repeat wait() until player.Character
local character = player.Character
local hrp = character:WaitForChild("HumanoidRootPart")

wait(1)

local Net = svc.RS.Packages._Index["sleitnick_net@0.2.0"].net

local Window = WindUI:CreateWindow({
    Title = "LS Hub - Fish It",
    Author = "developed by Louissxe",
    Folder = "LSHub-FishIt",
    Icon = "rbxassetid://129609617845057",
    Transparent = true,
    IconSize = 24,
    NewElements = true,
    Size = UDim2.fromOffset(500, 400),
    ToggleKey = Enum.KeyCode.G,
    User = {
        Enabled = true,
        Anonymous = true,
    },
    HideSearchBar = true,
})

local Fish10 = Window:Tab({
    Title = "Fishing",
    Icon = "bird",
})

local Running = false

_G.DelayComplete = 0.03
_G.DelayCycle = 2

Fish10:Input({
    Title = "Delay Complete",
    Description = "Delay spam charge+start",
    Value = "0.03",
    Callback = function(v)
        _G.DelayComplete = tonumber(v) or 0.03
    end,
})

Fish10:Input({
    Title = "Delay Cycle", 
    Description = "Delay catch/cancel",
    Value = "2",
    Callback = function(v)
        _G.DelayCycle = tonumber(v) or 2
    end,
})

Fish10:Toggle({
    Title = "Auto Fish",
    Default = false,
    Callback = function(s)
        Running = s
        
        if s then
            spawn(function()
                local EquipTool = Net["RE/EquipToolFromHotbar"]
                local ChargeRod = Net["RF/ChargeFishingRod"]
                local StartMini = Net["RF/RequestFishingMinigameStarted"]
                
                pcall(function() EquipTool:FireServer(1) end)
                wait(0.5)
                
                while Running do
                    local now = workspace:GetServerTimeNow()
                    
                    spawn(function()
                        pcall(function()
                            ChargeRod:InvokeServer(now)
                        end)
                    end)
                    
                    spawn(function()
                        pcall(function()
                            StartMini:InvokeServer(-1, 1, now)
                        end)
                    end)
                    
                    wait(_G.DelayComplete)
                end
            end)
            
            spawn(function()
                local CatchFish = Net["RF/CatchFishCompleted"]
                local CancelFish = Net["RF/CancelFishingInputs"]
                
                wait(1)
                
                local toggle = false
                
                while Running do
                    if toggle then
                        pcall(function()
                            CancelFish:InvokeServer()
                        end)
                    else
                        pcall(function()
                            CatchFish:InvokeServer()
                        end)
                    end
                    
                    toggle = not toggle
                    wait(_G.DelayCycle)
                end
            end)
        end
    end,
})

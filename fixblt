local WindUI = loadstring(game:HttpGet("https://raw.githubusercontent.com/Footagesus/WindUI/main/dist/main.lua"))()

wait(2)

local svc = {
    Players = game:GetService("Players"),
    RS = game:GetService("ReplicatedStorage"),
}

local player = svc.Players.LocalPlayer

repeat wait() until player.Character
local character = player.Character
local hrp = character:WaitForChild("HumanoidRootPart")

wait(1)

local Net = svc.RS.Packages._Index["sleitnick_net@0.2.0"].net

local Window = WindUI:CreateWindow({
    Title = "LS Hub - Fish It",
    Author = "developed by Raditya",
    Folder = "LSHub-FishIt",
    Icon = "rbxassetid://129609617845057",
    Transparent = true,
    IconSize = 24,
    NewElements = true,
    Size = UDim2.fromOffset(500, 400),
    ToggleKey = Enum.KeyCode.G,
    User = {
        Enabled = true,
        Anonymous = true,
    },
    HideSearchBar = true,
})

local Fish10 = Window:Tab({
    Title = "Fishing",
    Icon = "bird",
})

local Running = false

_G.DelayComplete = 0.05
_G.DelayCycle = 1

Fish10:Input({
    Title = "Delay Complete",
    Description = "Delay spam (recommend: 0.03-0.1)",
    Value = "0.05",
    Callback = function(v)
        _G.DelayComplete = tonumber(v) or 0.05
    end,
})

Fish10:Input({
    Title = "Delay Cycle", 
    Description = "Delay catch/cancel (recommend: 0.5-2)",
    Value = "1",
    Callback = function(v)
        _G.DelayCycle = tonumber(v) or 1
    end,
})

Fish10:Toggle({
    Title = "Auto Fish",
    Default = false,
    Callback = function(s)
        Running = s
        
        if s then
            spawn(function()
                local EquipTool = Net["RE/EquipToolFromHotbar"]
                local ChargeRod = Net["RF/ChargeFishingRod"]
                local StartMini = Net["RF/RequestFishingMinigameStarted"]
                local CatchFish = Net["RF/CatchFishCompleted"]
                local CancelFish = Net["RF/CancelFishingInputs"]
                
                pcall(function() EquipTool:FireServer(1) end)
                wait(0.5)
                
                while Running do
                    for i = 1, 30 do
                        if not Running then break end
                        
                        local now = workspace:GetServerTimeNow()
                        
                        pcall(function()
                            ChargeRod:InvokeServer(now)
                        end)
                        
                        wait(_G.DelayComplete / 2)
                        
                        pcall(function()
                            StartMini:InvokeServer(-1, 1, now)
                        end)
                        
                        wait(_G.DelayComplete / 2)
                    end
                    
                    wait(0.1)
                    
                    pcall(function()
                        CatchFish:InvokeServer()
                    end)
                    
                    wait(_G.DelayCycle)
                    
                    if not Running then break end
                    
                    pcall(function()
                        CancelFish:InvokeServer()
                    end)
                    
                    wait(_G.DelayCycle)
                end
            end)
        end
    end,
})
